package eu.davidfranco.TwitterBot;

public class TwitterBot {
	public static void main(String[] args){
		UserProfile p = new UserProfile("Pollitoyeye");
		for(Message s : p.getMessages()){
			System.out.println("Message date: " + s.getFormatedDate());
			System.out.println("Message content: \n" + s.getContent());
		}
	}
}
