package eu.davidfranco.TwitterBot;


public class TwitterManager {
	public static void startSearchThread(String[] keyWords){
		new Thread(new Runnable() {
		    public void run() {
		    	try {
					wait(1000);
					//System.out.println("Test");
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
		    }
		}).start();;
		
	}
}
